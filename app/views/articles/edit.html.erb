<div>
    <h2>Article Edit Form</h2><br/>

    <%= form_with(model: @article) do |f| %> 
        <%= f.label :title %>
        <%= f.text_field :title %> <br/>
        <%= f.label :note %>
        <%= f.text_field :note %> <br/>
        <%= f.label :Body %>
        <%= f.text_area :body, size: "60x10" %> <br/>
        <%= f.label :Tag %>
        <%= f.text_field :tag_list %><br /><br />
        <%= f.submit %> <br/>
    <% end %>
<div>